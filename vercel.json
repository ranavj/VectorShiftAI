{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "api/**.py": {
      "runtime": "python3.11"
    }
  },
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "backend/main.py"
    },
    {
      "source": "/:match*",
      "destination": "/index.html"
    }
  ]
}